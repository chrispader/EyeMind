{
  "name": "DCR Graph diagram",
  "uri": "http://tk/schema/dcr",
  "prefix": "dcr",
  "xml": {
    "tagAlias": "lowerCase"
  },
  "types": [
    {
      "name": "BoardElement",
      "isAbstract": true,
      "properties": [
        {
          "name": "id",
          "isAttr": true,
          "type": "String",
          "isId": true
        }
      ]
    },
    {
      "name": "Event",
      "superClass": [
        "BoardElement"
      ],
      "properties": [
        {
          "name": "role",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "description",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "included",
          "isAttr": true,
          "type": "Boolean"
        },
        {
          "name": "executed",
          "isAttr": true,
          "type": "Boolean"
        },
        {
          "name": "pending",
          "isAttr": true,
          "type": "Boolean"
        }
      ]
    },
    {
      "name": "Nesting",
      "superClass": [
        "BoardElement"
      ],
      "properties": [
        {
          "name": "role",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "description",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "boardElements",
          "type": "BoardElement",
          "isMany": true
        }
      ]
    },
    {
      "name": "SubProcess",
      "superClass": [
        "BoardElement"
      ],
      "properties": [
        {
          "name": "description",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "boardElements",
          "type": "BoardElement",
          "isMany": true
        },
        {
          "name": "included",
          "isAttr": true,
          "type": "Boolean"
        },
        {
          "name": "executed",
          "isAttr": true,
          "type": "Boolean"
        },
        {
          "name": "pending",
          "isAttr": true,
          "type": "Boolean"
        },
        {
          "name": "multi-instance",
          "isAttr": true,
          "type": "Boolean"
        }
      ]
    },
    {
      "name": "Relation",
      "superClass": [
        "BoardElement"
      ],
      "properties": [
        {
          "name": "type",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "sourceRef",
          "isAttr": true,
          "isReference": true,
          "type": "Event"
        },
        {
          "name": "targetRef",
          "isAttr": true,
          "isReference": true,
          "type": "Event"
        }
      ]
    },
    {
      "name": "DcrGraph",
      "superClass": [
        "RootElement"
      ],
      "properties": [
        {
          "name": "boardElements",
          "isMany": true,
          "type": "BoardElement"
        }
      ]
    },
    {
      "name": "TextBox",
      "superClass": [
        "BoardElement"
      ]
    },
    {
      "name": "RootElement",
      "isAbstract": true,
      "superClass": [
        "BoardElement"
      ]
    },
    {
      "name": "Definitions",
      "superClass": [
        "BoardElement"
      ],
      "properties": [
        {
          "name": "targetNamespace",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "expressionLanguage",
          "default": "http://www.w3.org/1999/XPath",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "typeLanguage",
          "default": "http://www.w3.org/2001/XMLSchema",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "rootElements",
          "type": "RootElement",
          "isMany": true
        },
        {
          "name": "rootBoards",
          "isMany": true,
          "type": "dcrDi:DcrRootBoard"
        },
        {
          "name": "exporter",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "exporterVersion",
          "isAttr": true,
          "type": "String"
        }
      ]
    }
  ]
}